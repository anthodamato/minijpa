SKIP :
{
  " "
| "\t"
| "\n"
| "\r"
}

TOKEN [IGNORE_CASE]:
{
 <AS:        "AS">
|<ABS:       "ABS">
|<ALL:       "ALL">
|<AND:       "AND">
|<ANY:       "ANY">
|<ASC:       "ASC">
|<AVG:       "AVG">
|<BETWEEN:      "BETWEEN">
|<BOTH:      "BOTH">
