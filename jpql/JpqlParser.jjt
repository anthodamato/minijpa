|<CASE:      "CASE">
|<COALESCE:  "COALESCE">
|<CONCAT:    "CONCAT">
|<COUNT:     "COUNT">
|<CURRENT_DATE:        "CURRENT_DATE">
|<CURRENT_TIME:        "CURRENT_TIME">
|<CURRENT_TIMESTAMP:   "CURRENT_TIMESTAMP">
|<DESC:      "DESC">
|<DISTINCT:  "DISTINCT">
|<ELSE:      "ELSE">
|<EMPTY:       "EMPTY">
|<END:       "END">
|<ENTRY:     "ENTRY">
|<ESCAPE:     "ESCAPE">
|<EXISTS:     "EXISTS">
|<FALSE:     "FALSE">
|<FETCH:     "FETCH">
