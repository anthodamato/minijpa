|<NULL:      "NULL">
|<NULLIF:    "NULLIF">
|<OBJECT:        "OBJECT">
|<OF:        "OF">
|<OR:        "OR">
|<ORDER:        "ORDER">
|<OUTER:     "OUTER">
|<SELECT:    "SELECT">
|<SIZE:      "SIZE">
|<SOME:      "SOME">
|<SQRT:      "SQRT">
|<SUBSTRING: "SUBSTRING">
|<SUM:       "SUM">
|<THEN:      "THEN">
|<TRAILING:  "TRAILING">
|<TREAT:     "TREAT">
|<TRIM:      "TRIM">
|<TRUE:      "TRUE">
|<TYPE:      "TYPE">
|<UPPER:     "UPPER">
|<WHEN:      "WHEN">
|<WHERE:     "WHERE">
|<VALUE:     "VALUE">
}

// 'identification_variable' replaced by NAME
// 'single_valued_object_field' replaced by NAME

TOKEN :
{
  <NAME: ["a"-"z","A"-"Z","_"] ( ["a"-"z","A"-"Z","_","0"-"9"] )* >
|
  <COMMA: ",">
|
  <DOT:   ".">
|
  <OPEN_BRACKET: "(">
|
  <CLOSE_BRACKET: ")">
| 
  <STRING_LITERAL: "'" ( (~["'","\n","\r"]) | ("''") )* "'">
|
  <NUMERIC_LITERAL: ( ["0"-"9"] )+ >
|
  <INPUT_PARAMETER: "?" <NUMERIC_LITERAL> | ":" <NAME> >
|
  <GT:    ">" >
| <GE:    ">=" >
| <LT:    "<" >
| <LE:    "<=" >
| <NE:    "<>" >
| <EQ:    "=" >
}

