|<FUNCTION:  "FUNCTION">
|<GROUP:     "GROUP">
|<HAVING:    "HAVING">
|<INDEX:     "INDEX">
|<INNER:     "INNER">
|<IN:        "IN">
|<IS:        "IS">
|<JOIN:      "JOIN">
|<KEY:       "KEY">
|<LEADING:   "LEADING">
|<LEFT:      "LEFT">
|<LENGTH:        "LENGTH">
|<LIKE:      "LIKE">
|<LOCATE:        "LOCATE">
|<LOWER:     "LOWER">
|<MEMBER:    "MEMBER">
|<MIN:       "MIN">
